{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "LinkDotNet.NCronJob": "Trace"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "SingleLine": true,
                "TimestampFormat": "HH:mm:ss ",
                "IncludeScopes": true
            }
        }
    }
}
